vars
  z1;z2;f1;f2;g1;g2;b1;b2

rules
  z1,g1,!b1 -> !g1;
  z2,g2,!b2 -> !g2;
  g1,f1,!b1 -> !f1;
  g2,f2,!b2 -> !f2;

  b1,g1 -> !g1,!b1,g2,b2;
  b2,g2 -> !g2,!b2,g1,b1;
  b1,f1 -> !f1,!b1,f2,b2;
  b2,f2 -> !f2,!b2,f1,b1;
  b1,z1 -> !z1,!b1,z2,b2;
  b2,z2 -> !z2,!b2,z1,b1;
  b1 -> !b1,b2;
  b2 -> !b2,b1

init
  z1,f1,g1,b1
  
check
  %z.((f2 & (z2 & g2)) | <>:z)



